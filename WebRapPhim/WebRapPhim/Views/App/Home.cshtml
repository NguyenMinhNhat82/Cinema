@{
    ViewBag.Title = "Home";
    AppXemPhimEntities1 db = new AppXemPhimEntities1();
    var ListSuatChieu = db.Film.ToList();
    List<Film> nowShowing = new List<Film>();
    foreach (Film i in ListSuatChieu) {
        if (Service.getDateSubtract(System.DateTime.Parse(i.ngay_cong_chieu.ToString()), DateTime.Now) < 10 && Service.getDateSubtract(DateTime.Now, System.DateTime.Parse(i.ngay_cong_chieu.ToString()))< 10) {
            nowShowing.Add(i);
